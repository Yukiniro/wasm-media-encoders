{
  "name": "wasm-media-encoders",
  "version": "0.5.1",
  "main": "dist/index.js",
  "module": "dist/es/index.mjs",
  "browser": "dist/browser/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arseneyr/wasm-media-encoders"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "wasm"
  ],
  "scripts": {
    "build:encoders": "rimraf \"src/wasm/build/* wasm\" && docker run --rm -it -e NODE_ENV -v $(pwd):/src -v emsdk_cache:/emsdk_portable/.data -w /src/src/wasm trzeci/emscripten:1.39.17-upstream make && node scripts/postprocess_wasm",
    "clean:encoders": "docker run --rm -it -v $(pwd):/src -w /src/src/wasm trzeci/emscripten:1.39.17-upstream make clean && docker volume rm emsdk_cache",
    "build:js": "rimraf \"dist/**/*\" && rollup -c",
    "build": "cross-env-shell NODE_ENV=development \"npm run build:encoders && npm run build:js\"",
    "build:prod": "cross-env-shell NODE_ENV=production \"yarn build:encoders && yarn build:js\"",
    "clean": "rimraf \"dist src/wasm/dist wasm\" && yarn clean:encoders",
    "test": "jest",
    "prepare": "npm run clean && npm run build:prod"
  },
  "dependencies": {
    "@types/emscripten": "^1.39.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@rollup/plugin-babel": "^5.0.2",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-url": "^5.0.0",
    "@types/jest": "^25.2.3",
    "copyfiles": "^2.3.0",
    "cross-env": "^7.0.2",
    "dts-bundle-generator": "^4.3.0",
    "jest": "^26.0.1",
    "jest-fetch-mock": "^3.0.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.9",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-ts": "^1.2.24",
    "ts-jest": "^26.0.0",
    "ts-transformer-minify-privates": "^0.3.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5",
    "wav": "^1.0.2"
  }
}
