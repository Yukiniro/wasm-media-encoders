{
  "name": "wasm-encoders",
  "version": "0.4.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:encoders": "\"docker run --rm -it -e NODE_ENV -v %cd%:/src -v emsdk_cache:/emsdk_portable/.data -w /src/src/wasm trzeci/emscripten:1.39.10-upstream make\" && node src/wasm/patch",
    "clean:encoders": "docker run --rm -it -v %cd%:/src -w /src/src/wasm trzeci/emscripten:1.39.10-upstream make clean",
    "build:js": "rollup -c",
    "build": "cross-env NODE_ENV=development npm run build:encoders && rollup -c",
    "build:prod": "cross-env NODE_ENV=production npm run build:encoders",
    "watch:encoders": "onchange \"src/wasm/**/*\" -e \"src/wasm/dist/**/*\" -- npm run build:encoders",
    "watch:js": "onchange \"src/wasm/dist/**/*\" \"src/**/*.ts\" -- npm run build:js"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^2.3.2",
    "@rollup/plugin-typescript": "^4.1.2",
    "@types/emscripten": "^1.39.4",
    "cross-env": "^7.0.2",
    "onchange": "^7.0.2",
    "rollup": "^2.10.9",
    "rollup-plugin-terser": "^6.1.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  }
}
